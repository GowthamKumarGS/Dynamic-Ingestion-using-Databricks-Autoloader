{
  "name": "Bronze_Ingestion",
  "email_notifications": {
    "no_alert_for_skipped_runs": false
  },
  "webhook_notifications": {},
  "timeout_seconds": 0,
  "max_concurrent_runs": 1,
  "tasks": [
    {
      "task_key": "Parameters",
      "run_if": "ALL_SUCCESS",
      "notebook_task": {
        "notebook_path": "/Workspace/Flights_data_dbproject/Parameters",
        "source": "WORKSPACE"
      },
      "timeout_seconds": 0,
      "email_notifications": {},
      "webhook_notifications": {}
    },
    {
      "task_key": "Incremental_Ingestion",
      "depends_on": [
        {
          "task_key": "Parameters"
        }
      ],
      "run_if": "ALL_SUCCESS",
      "for_each_task": {
        "inputs": "{{tasks.Parameters.values.output_key}}",
        "task": {
          "task_key": "Incremental_Ingestion_iteration",
          "run_if": "ALL_SUCCESS",
          "notebook_task": {
            "notebook_path": "/Workspace/Flights_data_dbproject/BronzeLayer",
            "base_parameters": {
              "src": "{{input.src}}"
            },
            "source": "WORKSPACE"
          },
          "timeout_seconds": 0,
          "email_notifications": {},
          "webhook_notifications": {}
        }
      },
      "timeout_seconds": 0,
      "email_notifications": {}
    }
  ],
  "queue": {
    "enabled": true
  },
  "performance_target": "PERFORMANCE_OPTIMIZED",
  "run_as": {
    "user_name": "gowthamkgsainaga@gmail.com"
  }
}
